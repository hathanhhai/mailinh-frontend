{
  "name": "landing-page-builder",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p typecheck \"build-only -- --mode testing {@}\" --",
    "build:dev": "run-p typecheck \"build-only -- --mode development {@}\" --",
    "build:test": "run-p typecheck \"build-only -- --mode testing {@}\" --",
    "build:stg": "run-p typecheck \"build-only -- --mode staging {@}\" -- ",
    "build:prod": "run-p typecheck \"build-only -- --mode production {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "typecheck": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "format": "prettier --write src/",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=18.3.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./cz.config.js"
    }
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^42.0.2",
    "@ckeditor/ckeditor5-vue": "^6.0.0",
    "@vueuse/core": "^10.11.0",
    "axios": "^1.7.2",
    "codemirror": "^5.65.17",
    "codemirror-editor-vue3": "^2.7.0",
    "conventional-changelog-cli": "^5.0.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "scss-loader": "^0.0.1",
    "slugify": "^1.6.6",
    "vue": "^3.4.31",
    "vue-dompurify-html": "^5.1.0",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.0",
    "vue3-quill": "^0.3.1",
    "xlsx": "^0.18.5",
    "xlsx-js-style": "^1.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.3.0",
    "@iconify/vue": "^4.1.2",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/codemirror": "^5.60.15",
    "@types/node": "^20.14.9",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "naive-ui": "^2.38.2",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.5.2",
    "unplugin-auto-import": "^0.18.0",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.3.3",
    "vite-plugin-vue-devtools": "^7.3.5",
    "vue-tsc": "^2.0.22"
  }
}
